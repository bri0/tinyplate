{
  "name": "tinyplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass --include-path scss scss/main.scss public/css/main.css",
    "build-js": "babel client/indexes --out-dir public/js/bundle --presets es2015-ie",
    "start": "nodemon --ext scss,js --ignore public/js/bundle --exec \"npm run build-css; npm run build-js; node ./bin/www\""
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1"
  }
}
